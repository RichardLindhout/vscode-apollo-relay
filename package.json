{
  "name": "vscode-apollo-relay",
  "version": "1.1.1",
  "description": "Simple configuration of vscode-apollo for Relay projects.",
  "keywords": [
    "apollo",
    "graphql",
    "relay",
    "vscode"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/relay-tools/vscode-apollo-relay",
  "homepage": "https://github.com/relay-tools/vscode-apollo-relay#readme",
  "author": {
    "name": "Eloy DurÃ¡n",
    "email": "eloy.de.enige@gmail.com",
    "url": "https://github.com/alloy"
  },
  "license": "MIT",
  "scripts": {
    "prepublish": "yarn run build",
    "lint": "tslint -p ./tsconfig.json --type-check",
    "test": "jest",
    "prebuild": "rm -rf ./dist",
    "build": "tsc -p ./tsconfig.build.json && cp -R ./src/dependencies ./dist && yarn build:update-require-sources",
    "build:update-require-sources": "yarn build:update-require-sources:suggestionList && yarn build:update-require-sources:didYouMean && yarn build:update-require-sources:graphql && yarn build:update-require-sources:apolloValidation",
    "build:update-require-sources:graphql": "jscodeshift --transform=./scripts/update-require-source.ts --extensions=js --rename-from='graphql' --rename-to=./dist/dependencies/graphql.js ./dist",
    "build:update-require-sources:suggestionList": "jscodeshift --transform=./scripts/update-require-source.ts --extensions=js --rename-from='graphql/jsutils/suggestionList' --rename-to=./dist/dependencies/suggestionList.js ./dist",
    "build:update-require-sources:didYouMean": "jscodeshift --transform=./scripts/update-require-source.ts --extensions=js --rename-from='graphql/jsutils/didYouMean' --rename-to=./dist/dependencies/didYouMean.js ./dist",
    "build:update-require-sources:apolloValidation": "jscodeshift --transform=./scripts/update-require-source.ts --extensions=js --rename-from='apollo-language-server/lib/errors/validation' --rename-to=./dist/dependencies/apolloValidation.js ./dist",
    "release": "standard-version"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/jscodeshift": "^0.6.3",
    "@types/node": "^12.7.4",
    "apollo-language-server": "^1.15.0",
    "jest": "24.9.0",
    "jscodeshift": "^0.6.4",
    "prettier": "1.18.2",
    "relay-compiler": "^5.0.0",
    "relay-compiler-language-typescript": "^4.7.0",
    "relay-config": "^5.0.0",
    "standard-version": "7.0.0",
    "ts-jest": "24.0.2",
    "tslint": "5.19.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.6.2"
  },
  "peerDependencies": {
    "relay-compiler": "^5.0.0",
    "relay-config": "^5.0.0"
  },
  "engines": {
    "node": ">= 8"
  },
  "files": [
    "/dist/**/*"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true
  }
}
